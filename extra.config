#-----------要安装的插件-----------#
# Argon主题
CONFIG_PACKAGE_luci-theme-argonne=y
CONFIG_PACKAGE_luci-app-argonne-config=y
# 文件传输助手
CONFIG_PACKAGE_luci-app-filetransfer=y
# SSR PLUS+
CONFIG_PACKAGE_luci-app-ssr-plus=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Xray=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Trojan=y
# 内网穿透客户端
CONFIG_PACKAGE_luci-app-frpc=y
# 优选DNS
CONFIG_PACKAGE_luci-app-smartdns=y
# CONFIG_PACKAGE_luci-app-mosdns=y
# web管理
CONFIG_PACKAGE_luci-app-uhttpd=y
# 网络共享
CONFIG_PACKAGE_luci-app-samba=y
# FTP服务器
CONFIG_PACKAGE_luci-app-vsftpd=y
# qos限速
CONFIG_PACKAGE_luci-app-eqos=y
CONFIG_PACKAGE_luci-app-sqm=y
# 负载均衡
CONFIG_PACKAGE_luci-app-mwan3=y
# 多拨虚拟网卡
CONFIG_PACKAGE_luci-app-syncdial=y
# Turbo ACC 网络加速
CONFIG_PACKAGE_luci-app-turboacc=y
# 网络监控
CONFIG_PACKAGE_luci-app-nlbwmon=y
CONFIG_PACKAGE_luci-app-wrtbwmon=y
# 网络速度测试
CONFIG_PACKAGE_luci-app-speedtest-web=y
#-----------不要安装的插件-----------#
# 访问时间控制
CONFIG_PACKAGE_luci-app-accesscontrol=n
# Bypass
CONFIG_PACKAGE_luci-app-bypass=n
# 动态DNS
CONFIG_PACKAGE_ddns-scripts=n
CONFIG_PACKAGE_ddns-scripts_aliyun=n
CONFIG_PACKAGE_ddns-scripts_dnspod=n
CONFIG_PACKAGE_luci-app-ddns=n
# 通用即插即用UPnP（端口自动转发）
CONFIG_PACKAGE_luci-app-upnp=n
# KMS服务器设置
CONFIG_PACKAGE_luci-app-vlmcsd=n
# WOL网络唤醒
CONFIG_PACKAGE_luci-app-wol=n
